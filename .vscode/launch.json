{
    "version": "0.2.0",
    "configurations": [
        {
            "program": "${workspaceRoot}/build/MyExample",
            "name": "Debug",
            "type": "cppdbg",
            "request": "launch",
            "args": [],
            "externalConsole": false,
            "stopAtEntry": false,
            "target": "${workspaceRoot}/main.cpp",
            "cwd": "${workspaceRoot}",
            "MIMode": "gdb",
            "preLaunchTask": "build",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}